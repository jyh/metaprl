#
# Library files
#
MPFILES[] =
    m_prec
    m_int
    m_rawint
    m_rawfloat
    m_set
    m_fir
    m_fir_term

#
# At least m_fir_term needs to be compiled with -rectypes
#
OCAMLFLAGS += -rectypes

#
# Build the theory
#
Theory($(MPFILES))

#
# Clean up
#
clean:
    $(CLEAN)

all: theory$(LIB_SUFFIX)
