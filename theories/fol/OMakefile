	fol_itt_or\
	fol_itt_implies\
	fol_itt_prop\
	cfol_magic\
	cfol_theory\
	cfol_itt_base\
	cfol_itt_and\
	cfol_itt_all

Theory($(MPFILES))

#
# Documentation
#
PRINT_THEORIES =\
    fol_theory

TheoryDocumentInstall(fol, $(PRINT_THEORIES))

#
# Clean up
#
clean:
    $(CLEAN) theory.tex theory-body.tex theory_print.ml

all: theory$(LIB_SUFFIX)
