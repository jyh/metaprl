THEORYNAME = itt_extensions
THEORYDESCR = Extensions to Computational Type Theory (Some are Experimental)

THEORY_DEPENCIES[] +=
    itt/core
    itt/extensions/rfun

# Library files
MPFILES[] =
   itt_eta
   itt_pairwise
   itt_pairwise2
   itt_antiquotient
   ctt_markov

Theory($(MPFILES))

#
# Clean up
#
clean:
    $(CLEAN)

all: theory$(LIB_SUFFIX)
