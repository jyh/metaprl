THEORYNAME = itt_reflection_experimental
THEORYDESCR = Syntax Reflection for Computational Type Theory - Work in progress

OCAMLINCLUDES[] +=
   $(SUPPORT_DIRS)
   ../../../base
   ../../core
   ../core

DIRS[] =
   jyh

# Library files
EXPERIMENTAL_FILES[] =

.SUBDIRS: $(DIRS)
    include OMakefile
    EXPERIMENTAL_FILES[] += $(file $(MPFILES))
    export EXPERIMENTAL_FILES

MPFILES[] =
   itt_hoas_lang
   itt_hoas_bterm
   itt_hoas_util
   itt_hoas_sequent
   itt_hoas_olang
   $(EXPERIMENTAL_FILES)

Theory($(MPFILES))

#
# Clean up
#
clean:
    $(CLEAN)

all: theory$(LIB_SUFFIX)
