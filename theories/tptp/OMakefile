OCAMLINCLUDES +=\
	$(addprefix -I , $(SUPPORT_DIRS))\
	-I ../itt\
	-I ../base

MLZFILES =\
	tptp_type\
	tptp_parse

#
# Generated files
#
OCamlGeneratedFiles(tptp_lex.ml tptp_parse.ml)

#
# Ocamldep usually forgests this dependency
#
tptp_lex.cmi: tptp_parse.cmi

MPFILES =\
	tptp_type\
	tptp_parse\
	tptp_lex\
	tptp\
	tptp_load\
	tptp_cache\
	tptp_prove

Theory($(MPFILES))

#
# Clean up
#
clean:
    $(CLEAN)

all: theory$(LIB_SUFFIX)
