OCAMLINCLUDES[] +=
    $(SUPPORT_DIRS)
    ../itt
    ../base

MLZFILES[] =
    tptp_type
    tptp_parse

MPFILES[] =
    tptp_type
    tptp_parse
    tptp_lex
    tptp
    tptp_load
    tptp_cache
    tptp_prove

#
# Generated files
#
OCamlGeneratedFiles(tptp_lex.ml tptp_parse.ml)

#
# The theory
#
Theory($(MPFILES))

#
# Clean up
#
clean:
    $(CLEAN) tptp_lex.ml

all: theory$(LIB_SUFFIX)
